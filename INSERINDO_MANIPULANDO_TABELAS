	/*
	SELECIONA CAMPOS COMO NOME_ALUNO , IDADE , CURSO , RA DE ALUNOS
	*/
	
	-- FILTRAGEM
	
	
SELECT NOME, SEXO FROM ALUNOS
WHERE SEXO = 'M';

DESEJO UMA LISTA DE ALUNOS FEMININOS
SELECT NOME, SEXO FROM ALUNOS
WHERE SEXO = 'F';

	*/
	--CORRIGINDO CAMPO ERRADO AONDE O ID 15 QUE É O DA MARIANA ESTA MARCADO COM O SEXO MASCULINO
	
UPDATE ALUNOS
SET SEXO='F'
WHERE IDALUNO='15';
	
-- CONTANDO QUANTOS ALUNOS TEM NA TABELA
-- PROJETANDO "QUANTIDADE " E MOSTRANDO O TOTAL DE ALUNOS INSERIDOS

SELECT COUNT(*) AS "QUANTIDADE" FROM ALUNOS;


--  SELECIONANDO TODOS OS ALUNOS DA CIDADE DE CAMPINAS
SELECT NOME_ALUNO,SEXO FROM ALUNOS
WHERE CIDADE ='CAMPINAS';


-- CONTANDO TODOS OS ALUNOS E PROJETANDO A COLUNA QUANTIDADE DE ALUNOS
SELECT COUNT(*) AS "QUANTIDADE DE ALUNOS" FROM ALUNOS
WHERE CIDADE = 'CAMPINAS';

-- APAGANDO BANCO DE DADOS 

DROP DATABASE (NOME_DO_BANCO);

-- APAGANDO TABELA

DROP TABLE ( NOME_TABELA);






-- Atualizando CAMPOS



UPDATE TURMA
SET TURMA = 'D'
WHERE ID_ALUNO = 6;

UPDATE TURMA
SET TURMA = 'B'
WHERE ID_ALUNO = 7;



UPDATE TURMA
SET TURMA ='B'
WHERE ID_ALUNO = 8;

UPDATE TURMA
SET TURMA = 'A'
WHERE ID_ALUNO = 10;


UPDATE TURMA
SET TURMA = 'A'
WHERE ID_ALUNO = 11;


UPDATE TURMA
SET TURMA = 'C'
WHERE ID_ALUNO = 12;


UPDATE TURMA
SET TURMA = 'C'
WHERE ID_ALUNO = 13;


UPDATE TURMA
SET TURMA = 'B'
WHERE ID_ALUNO = 15 ;

     UPDATE TURMA
     SET TURMA = 'B'
     WHERE ID_ALUNO = 16;

-- BUSCANDO NOMES E SOBRENOMES

SELECT NOME_ALUNO , SEXO FROM ALUNOS
WHERE NOME_ALUNO LIKE 'DEFAVERE';


