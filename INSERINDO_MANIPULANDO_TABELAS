	--PARA SABER QUANTOS PROFESSORES HA NA TABELA BASTA NO FINAL DA SINTAXE ACRESCENTAR UM GROUP BY MAIS A COLUNA COMO ESSE EXEMPLO
	     FROM ALUNOS A
         INNER JOIN DISCIPLINAS D
         ON A.IDALUNO = D.ID_ALUNO
         INNER JOIN TURMA T
         ON A.IDALUNO = T.ID_ALUNO
         INNER JOIN PROFESSORES P
     ON A.IDALUNO = P.ID_ALUNO
     GROUP BY  NOME_PROFESSOR;
