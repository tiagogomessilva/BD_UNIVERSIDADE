/* TABELA DA VERDADE

AND OU OR

SE NA CONDIÇÃO AND TODAS AS DECLARACOES PRECISAM SER VERDADEIRAS PARA QUE SEJA EXIBIDO O RESULTADO CONFORME A SENTENÇA
NA SENTENÇA OR É AO CONTRARIO ,SALVO SE TODAS AS CONDIÇÕES FOREM FALSAS PARA QUE NÃO EXIBA NENHUM RESULTADO

OU
V,V = VERDADE
V,F = VERDADE
F,V = VERDADE
F,F = FALSO



AND 

V, V = VERDADE
F, V = FALSO
V, F = FALSO
F, F = FALSO
*/

SELECT NOME_ALUNO,SEXO FROM ALUNOS
     WHERE
     SEXO ='M' OR NOME_ALUNO = 'MATHIAS'

/*

NAO POSSUI NENHUM CLIENTE MATHIAS POREM EXIBIU TODAS AS INFORMAÇÕES DO SEXO MASCULINO
+------------------+------+
| NOME_ALUNO       | SEXO |
+------------------+------+
| CALEBE FERNANDES | M    |
| GAEL MARTINS     | M    |
| THOMAS HONORIO   | M    |
| EDUARDO LUZ      | M    |
| THIAGO FERRAZ    | M    |
| NATHAN SOUZA     | M    |
| CESAR CAVALCANTI | M    |
| BRYAN GOMES      | M    |
| CARLOS HENRIQUE  | M    |
| DONIZETE BRITO   | M    |
| DANIEL CORREIA   | M    |
| JOEL FERNANDES   | M    |
| RENATO TAKAMUNE  | M    |
| GILMAR CRUZ      | M    |
| JOAO PAULO       | M    |
| ALEX YOSHITAKE   | M    |
| PEDRO  GATTI     | M    |
| GAEL GOMES       | M    |
| JOAO DEFAVERE    | M    |
+------------------+------+
*/
SELECT NOME_ALUNO,NOME_ALUNO FROM ALUNOS
    WHERE
    NOME_ALUNO = 'SILVEIRA' OR NOME_ALUNO='EMERSON';
-- Empty set (0.00 sec)

--NESSA CONDIÇÃO NAO EXIBIU NENHUMA INFORMAÇÃO POIS NAO HA NA TABELA NINGUEM QUE POSSUA ESSE NOME
*/
SELECT NOME_ALUNO,SEXO FROM ALUNOS
    WHERE
    SEXO = 'F' AND NOME_ALUNO='DAIANE SILVA'
    -> ;
	/*
+--------------+------+
| NOME_ALUNO   | SEXO |
+--------------+------+
| DAIANE SILVA | F    |
+--------------+------+
*/
--SE UMA DESSAS CONDIÇÕES FOSSE FALSA NAO EXIBIRIA NENHUMA INFORMAÇÃO COMO OCORREU NO OR

SELECT NOME_ALUNO,SEXO FROM ALUNOS
WHERE
SEXO='F' AND NOME_ALUNO='DAIANE SOUZA';





*/

--CATEGORIZANDO MELHOR  QUANTIDADE DE INFORMAÇÕES 
-- SABER A QUANTIDADE DE TURMAS EM DISCIPLINAS

SELECT TURMA , COUNT(*) AS 'QUANTIDADE DE TURMAS ' FROM TURMA
GROUP BY TURMA;
/*
+-------+----------------------+
| TURMA | QUANTIDADE DE TURMAS |
+-------+----------------------+
| A     |                   24 |
| B     |                    5 |
| C     |                    3 |
| D     |                    2 |
+-------+----------------------+
*/


SELECT NOME_DISC, COUNT(*) AS 'QUANTIDADE DE DISCIPLINAS' FROM DISCIPLINAS
     GROUP BY NOME_DISC;
	 
	 
	 /*
 +------------------------+---------------------------+
| NOME_DISC              | QUANTIDADE DE DISCIPLINAS |
+------------------------+---------------------------+
| ALGEBRA LINEAR         |                         2 |
| BANCO DE DADOS         |                         1 |
| BIOQUIMICA             |                         2 |
| DESENHO TECNICO        |                         2 |
| DIREITO                |                         1 |
| ENGENHARIA SOFTWARE    |                         2 |
| ESTATISTICA            |                         3 |
| ESTRUTURA MATERIAIS    |                         7 |
| GESTAO                 |                         2 |
| MATEMATICA             |                         4 |
| ORIENTACAO OBJETOS     |                         2 |
| RECURSOS HUMANOS       |                         2 |
| REDES COMPUTADORES     |                         1 |
| SOCIOLOGIA             |                         2 |
| TECNOLOGIAS TRANSPORTE |                         2 |
+------------------------+---------------------------+
*/
SELECT CARGA_HORARIA, COUNT(*) AS 'QUANTIDADE DE CARGA HORARIA' FROM DISCIPLINAS
    -> GROUP BY CARGA_HORARIA;
	/*
+---------------+-----------------------------+
| CARGA_HORARIA | QUANTIDADE DE CARGA HORARIA |
+---------------+-----------------------------+
| 100           |                          15 |
| 50            |                           3 |
| 70            |                           7 |
| 80            |                           5 |
| 90            |                           5 |
+---------------+-----------------------------+
*/
 SELECT MODALIDADE, COUNT(*) AS 'MODALIDADE' FROM TURMA
     GROUP BY MODALIDADE;
/*
+-----------------+------------+
| MODALIDADE      | MODALIDADE |
+-----------------+------------+
| EAD             |          5 |
| SEMI-PRESENCIAL |          4 |
| PRESENCIAL      |         26 |
+-----------------+------------+

*/

-- JUNTANDO COLUNAS DE OUTRAS TABELAS EM UMA SÓ...
	
	 SELECT A.IDALUNO,A.NOME_ALUNO,A.SEXO,A.IDADE,A.CIDADE,A.CURSO,A.RA,
     D.NOME_DISC,D.CARGA_HORARIA,D.NOTA,D.FREQUENCIA,
	 T.MODALIDADE,T.PERIODO,T.TURMA,
     P.NOME_PROFESSOR
    
    FROM ALUNOS A
    INNER JOIN DISCIPLINAS D
    ON A.IDALUNO = D.ID_ALUNO
    INNER JOIN TURMA T
    ON A.IDALUNO = T.ID_ALUNO
    INNER JOIN PROFESSORES P
	ON A.IDALUNO = P.ID_ALUNO;
	 
